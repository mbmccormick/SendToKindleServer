@{
    ViewBag.Title = "Send a website to your Kindle";
}

<div class="form-horizontal">
    <h3>Send a website to your Kindle</h3>
    <hr />
</div>

<div>
    <p>There are currently @ViewBag.MessageCount message(s) in the queue waiting to be processed.</p>
    <br />
    <p>Dumping all messages from queue...</p>
    <br />
    <p>Successfully deleted @ViewBag.DeletedMessageCount message(s) from the queue.</p>
    <br />
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
